if (filters == 1) {
N <- 100
hann <- function(n) {
0.5 - 0.5 * cos(2 * pi * n / N)
}
hamming <- function(n) {
0.54 - 0.46 * cos(2 * pi * n / N)
}
blackman <- function(n) {
0.42 - 0.5 * cos(2 * pi * n / N) +
0.08 * cos(4 * pi * n / N)
}
nuttall <- function(n) {
0.36 - 0.49 * cos(2 * pi * n / N) +
0.14 * cos(4 * pi * n / N) - 0.01 * cos(6 * pi * n / N)
}
welch <- function(n) {
1 - ((n - N / 2) / (N / 2)) ^ 2
}
sine <- function(n) {
sin(pi * n / N)
}
triangular <- function(n) {
1 - abs((n - N / 2) / (N / 2))
}
rectangular <- function(n) {
1
}
